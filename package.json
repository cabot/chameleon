{
  "name": "base-l",
  "version": "0.16.0",
  "description": "A well structured top down base css framework meant for web apps",
  "main": "gulpfile.js",
  "xo": {
    "rules": {
      "quote-props": [
        "error",
        "always"
      ]
    },
    "env": [
      "es6",
      "browser",
      "node",
