  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hanakin/base-l/issues"
  },
  "homepage": "https://github.com/hanakin/base-l#readme",
  "devDependencies": {
    "del": "^2.2.0",
    "gulp": "4.0.0",
    "gulp-autoprefixer": "^3.1.0",
    "gulp-cssnano": "^2.1.3",
    "gulp-data": "^1.3.1",
    "gulp-nunjucks-render": "github:carlosl/gulp-nunjucks-render#master",
    "gulp-postcss": "^6.2.0",
    "gulp-rename": "1.3.0",
    "gulp-sass": "4.0.1",
    "gulp-sourcemaps": "^1.12.1",
    "gulp-stylefmt": "^1.1.0",
    "moment": "2.22.2",
